
<h2>Formulaire de synonyme</h2>

<form class="mot-form" method="post">
        <!-- mot -->
        <div>
            <label for="mot">Mot</label>
            <input type="text" id="nouveau-mot" name="mot" required value=<%= mot %>>
        </div>
        <!-- synonyme -->
        <div>
            <label for="synonyme-mot">Synonyme mot</label>
            <input type="text" id="synonyme-mot" name="synonyme" required>
        </div>
    <div><button type="submit">Ajouter</button></div>
</form>